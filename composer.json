{
  "name": "lukasjakobi/regular",
  "description": "A regular expression builder and interface for php's built in preg_* functions",
  "keywords": [
    "regex",
    "regex",
    "regular",
    "expression",
    "expressions"
  ],
  "homepage": "https://github.com/lukasjakobi/regular",
  "license": "MIT",
  "authors": [
    {
      "name": "Lukas Jakobi",
      "email": "lukas@jakobi.io",
      "homepage": "https://jakobi.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^8.0",
    "mbstring" : "latest"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "LukasJakobi\\Regular\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LukasJakobi\\Regular\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
